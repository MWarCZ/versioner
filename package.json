{
  "name": "versioner",
  "version": "1.0.1",
  "description": "",
  "main": "./dist/src/index.js",
  "bin": {
    "versioner": "./dist/src/cli.js"
  },
  "author": "MWarCZ",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MWarCZ/versioner.git"
  },
  "env": {
    "path": {
      "dist": "dist",
      "src": "src",
      "testout": "coverage"
    }
  },
  "scripts": {
    "env": "env | sort | grep ^npm_ | less",
    "start": "node $npm_package_bin_versioner",
    "prebuild": "$npm_execpath run build:clean",
    "build": "$npm_execpath run tsc",
    "build:clean": "rimraf $npm_package_env_path_dist",
    "lint": "tslint -c tslint.json -p tsconfig.json --format codeFrame",
    "lint:fix": "$npm_execpath run lint --fix",
    "test": "jest --config jest.config.json --forceExit --verbose --detectOpenHandles",
    "test:watch": "$npm_execpath run test -- --watchAll",
    "test:clean": "rimraf $npm_package_env_path_testout",
    "pretest:cov": "$npm_execpath run test:clean",
    "test:cov": "jest --config jest.config.json --forceExit --coverage --verbose --detectOpenHandles",
    "test:lint": "tslint -c tslint.json -p tsconfig.test.json --format codeFrame",
    "clean": "$npm_execpath run build:clean; $npm_execpath run test:clean;",
    "tsnode": "NODE_ENV='DEBUG' ts-node $npm_package_env_path_src/index.ts"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/minimist": "^1.2.0",
    "@types/node": "^12.7.2",
    "@types/semver": "^6.0.1",
    "jest": "^24.9.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-clean-code": "^0.2.9",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "js-logger": "^1.6.0",
    "minimist": "^1.2.0",
    "semver": "^6.3.0"
  }
}
